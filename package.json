{
  "name": "project-2",
  "version": "1.0.0",
  "description": "Fellowship user service",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "build": "npm install && tsc && npm prune --production && cp ./package.json ./build",
    "build:clean": "rm -rf ./build && npm install && tsc && npm prune --production && cp ./package.json ./build",
    "deploy": "node dist/index.js"
  },
  "author": "RchlRiedel",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.3",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.2.0",
    "@google-cloud/storage": "^5.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.3.0",
    "pg": "^8.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
